<div class = "form-data">
    <form action ="/users/create" method ="POST">
    
    <label for="txtFName">First Name</label>
    <input type="text" name="first" id="txtFName" autofocus required>

    <label for="txtLName">Last Name</label>
    <input type="text" name="last" id="txtLName" autofocus required>
    
    <label for="txtEmail">Email</label>
    <input type="email" required name="email" id ="txtEmail">
    
    <label for="txtZipCode">ZipCode</label>
    <input type="Number" name="zipCode" id="txtZipCode" required>

    <label for="txtPass">Password</label>
    <input type="password" name="password" id="txtPass" required>
    
    <button type="submit">Create</button>
    </form>
</div>